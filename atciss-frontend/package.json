                "async-mutex": "^0.5.0",
                "luxon": "^3.5.0",
                "react": "^18.2.0",
                "react-dom": "^19.0.0",
                "react-leaflet": "^4.2.1",
                "react-redux": "^9.1.2",
                "react-router": "^7.0.2",
                "theme-ui": "^0.17.1"
        },
        "devDependencies": {
                "@testing-library/dom": "^10.4.0",
                "@testing-library/jest-dom": "^6.6.3",
                "@testing-library/react": "^16.0.1",
                "@testing-library/user-event": "^14.5.2",
                "@types/leaflet": "^1.9.15",
                "@types/luxon": "^3.4.2",
                "@types/react": "^18.3.13",
